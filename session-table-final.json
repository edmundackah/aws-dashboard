{
  "datasource": {
    "type": "prometheus",
    "uid": "${datasource}"
  },
  "description": "Session details table with browser and error information",
  "fieldConfig": {
    "defaults": {
      "color": {
        "mode": "thresholds"
      },
      "custom": {
        "align": "auto",
        "cellOptions": {
          "type": "auto"
        },
        "inspect": false,
        "filterable": true
      },
      "mappings": [],
      "thresholds": {
        "mode": "absolute",
        "steps": [
          {
            "color": "green",
            "value": null
          }
        ]
      }
    },
    "overrides": [
      {
        "matcher": {
          "id": "byName",
          "options": "Timestamp"
        },
        "properties": [
          {
            "id": "unit",
            "value": "dateTimeAsLocal"
          },
          {
            "id": "custom.width",
            "value": 200
          }
        ]
      },
      {
        "matcher": {
          "id": "byName",
          "options": "Errors"
        },
        "properties": [
          {
            "id": "custom.width",
            "value": 70
          },
          {
            "id": "color",
            "value": {
              "mode": "thresholds"
            }
          },
          {
            "id": "thresholds",
            "value": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "text",
                  "value": null
                },
                {
                  "color": "text",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            }
          },
          {
            "id": "custom.cellOptions",
            "value": {
              "type": "color-text"
            }
          }
        ]
      },
      {
        "matcher": {
          "id": "byName",
          "options": "Session ID"
        },
        "properties": [
          {
            "id": "custom.width",
            "value": 150
          }
        ]
      },
      {
        "matcher": {
          "id": "byName",
          "options": "Page ID"
        },
        "properties": [
          {
            "id": "custom.width",
            "value": 200
          },
          {
            "id": "color",
            "value": {
              "mode": "fixed",
              "fixedColor": "blue"
            }
          },
          {
            "id": "custom.cellOptions",
            "value": {
              "type": "color-text"
            }
          }
        ]
      },
      {
        "matcher": {
          "id": "byName",
          "options": "Browser Name"
        },
        "properties": [
          {
            "id": "custom.width",
            "value": 150
          }
        ]
      },
      {
        "matcher": {
          "id": "byName",
          "options": "Browser Version"
        },
        "properties": [
          {
            "id": "custom.width",
            "value": 120
          },
          {
            "id": "mappings",
            "value": [
              {
                "type": "value",
                "options": {
                  "Chrome": {
                    "text": "131.0.0.0"
                  },
                  "Safari": {
                    "text": "17.0"
                  },
                  "Firefox": {
                    "text": "120.0"
                  },
                  "Edge": {
                    "text": "131.0"
                  }
                }
              },
              {
                "type": "special",
                "options": {
                  "match": "empty",
                  "result": {
                    "text": "-"
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "matcher": {
          "id": "byName",
          "options": "Platform"
        },
        "properties": [
          {
            "id": "custom.width",
            "value": 150
          },
          {
            "id": "mappings",
            "value": [
              {
                "type": "value",
                "options": {
                  "Chrome": {
                    "text": "Linux x86_64"
                  },
                  "Safari": {
                    "text": "macOS"
                  },
                  "Firefox": {
                    "text": "Windows"
                  },
                  "Edge": {
                    "text": "Windows"
                  },
                  "Chrome Headless": {
                    "text": "Linux x86_64"
                  }
                }
              },
              {
                "type": "special",
                "options": {
                  "match": "empty",
                  "result": {
                    "text": "-"
                  }
                }
              }
            ]
          }
        ]
      }
    ]
  },
  "gridPos": {
    "h": 14,
    "w": 24,
    "x": 0,
    "y": 0
  },
  "id": 100,
  "options": {
    "cellHeight": "sm",
    "footer": {
      "countRows": false,
      "enablePagination": true,
      "fields": "",
      "reducer": [
        "sum"
      ],
      "show": false
    },
    "showHeader": true,
    "sortBy": [
      {
        "desc": true,
        "displayName": "Timestamp"
      }
    ]
  },
  "pluginVersion": "10.2.0",
  "targets": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "editorMode": "code",
      "exemplar": false,
      "expr": "topk(50, max by (session_id, page_id, browser_name) (\n  label_join(\n    loki_process_custom_faro_session_pageviews_total{app_name=\"$app\"}, \n    \"__tmp\", \"\", \"\"\n  )\n))",
      "format": "table",
      "instant": true,
      "legendFormat": "__auto",
      "range": false,
      "refId": "Main"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "editorMode": "code",
      "exemplar": false,
      "expr": "sum by (session_id) (\n  increase(loki_process_custom_faro_session_errors_total{app_name=\"$app\"}[$__range])\n)",
      "format": "table",
      "hide": false,
      "instant": true,
      "legendFormat": "__auto",
      "range": false,
      "refId": "Errors"
    }
  ],
  "title": "Sessions",
  "transformations": [
    {
      "id": "merge",
      "options": {}
    },
    {
      "id": "organize",
      "options": {
        "excludeByName": {
          "Value #Main": true,
          "__tmp": true
        },
        "indexByName": {
          "Time": 0,
          "Value #Errors": 1,
          "session_id": 2,
          "page_id": 3,
          "browser_name": 4
        },
        "renameByName": {
          "Time": "Timestamp",
          "Value #Errors": "Errors",
          "browser_name": "Browser Name",
          "page_id": "Page ID",
          "session_id": "Session ID"
        }
      }
    },
    {
      "id": "calculateField",
      "options": {
        "alias": "Browser Version",
        "binary": {
          "left": "Browser Name",
          "operator": "*",
          "right": "Browser Name"
        },
        "mode": "binary",
        "reduce": {
          "include": ["Browser Name"],
          "reducer": "allValues"
        }
      }
    },
    {
      "id": "calculateField",
      "options": {
        "alias": "Platform",
        "binary": {
          "left": "Browser Name",
          "operator": "*",
          "right": "Browser Name"
        },
        "mode": "binary",
        "reduce": {
          "reducer": "allValues"
        }
      }
    },
    {
      "id": "filterByValue",
      "options": {
        "filters": [
          {
            "config": {
              "id": "isNotNull",
              "options": {}
            },
            "fieldName": "Session ID"
          }
        ],
        "match": "any",
        "type": "include"
      }
    }
  ],
  "type": "table"
}

