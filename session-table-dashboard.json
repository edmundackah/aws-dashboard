      "gridPos": {
        "h": 16,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "cellHeight": "sm",
        "footer": {
          "countRows": false,
          "enablePagination": true,
          "fields": "",
          "reducer": [
            "sum"
          ],
          "show": false
        },
        "showHeader": true,
        "sortBy": [
          {
            "desc": true,
            "displayName": "Timestamp"
          }
        ]
      },
      "pluginVersion": "10.2.0",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "expr": "topk(100, \n  max by (session_id, page_id, browser_name) (\n    loki_process_custom_faro_session_pageviews_total{app_name=\"$app\"}\n  )\n)",
          "format": "table",
          "instant": true,
          "refId": "SessionData"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "expr": "sum by (session_id) (\n  increase(loki_process_custom_faro_session_errors_total{app_name=\"$app\"}[$__range])\n) or (\n  group by (session_id)(\n    loki_process_custom_faro_session_events_total{app_name=\"$app\"}\n  ) * 0\n)",
          "format": "table",
          "instant": true,
          "refId": "ErrorCount"
        }
      ],
      "title": "Sessions",
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Value #SessionData": true
            },
            "indexByName": {
              "Time": 0,
              "Value #ErrorCount": 1,
              "session_id": 2,
              "page_id": 3,
              "browser_name": 4
            },
            "renameByName": {
              "Time": "Timestamp",
              "Value #ErrorCount": "Errors",
              "browser_name": "Browser Name",
              "page_id": "Page ID",
              "session_id": "Session ID"
            }
          }
        },
        {
          "id": "calculateField",
          "options": {
            "alias": "Browser Version",
            "mode": "constant",
            "reduce": {
              "reducer": "sum"
            }
          }
        },
        {
          "id": "calculateField",
          "options": {
            "alias": "Platform",
            "mode": "constant",
            "reduce": {
              "reducer": "sum"
            }
          }
        }
      ],
      "type": "table"
    }
  ],
  "refresh": "30s",
